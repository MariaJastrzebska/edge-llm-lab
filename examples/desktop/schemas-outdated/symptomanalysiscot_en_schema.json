{
  "$defs": {
    "IntensityEnum": {
      "enum": [
        "mild",
        "moderate",
        "severe"
      ],
      "title": "IntensityEnum",
      "type": "string"
    },
    "MedScreeningInputStatus": {
      "enum": [
        "incomplete",
        "complete",
        "skipped"
      ],
      "title": "MedScreeningInputStatus",
      "type": "string"
    },
    "MissingInfo": {
      "properties": {
        "field": {
          "description": "Missing information, e.g. 'height'",
          "maxLength": 50,
          "title": "Field",
          "type": "string"
        },
        "question": {
          "description": "Question to user, e.g. 'What is your height?'",
          "maxLength": 200,
          "title": "Question",
          "type": "string"
        }
      },
      "required": [
        "field",
        "question"
      ],
      "title": "MissingInfo",
      "type": "object"
    },
    "SymptomSetup": {
      "properties": {
        "name": {
          "anyOf": [
            {
              "maxLength": 100,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Symptom name - optional",
          "title": "Name"
        },
        "intensity": {
          "anyOf": [
            {
              "$ref": "#/$defs/IntensityEnum"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Symptom intensity - optional"
        },
        "duration": {
          "anyOf": [
            {
              "maxLength": 50,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Duration - optional",
          "title": "Duration"
        },
        "frequency": {
          "anyOf": [
            {
              "maxLength": 50,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Frequency - optional",
          "title": "Frequency"
        },
        "onset_date": {
          "anyOf": [
            {
              "format": "date",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Onset date - optional",
          "title": "Onset Date"
        }
      },
      "title": "SymptomSetup",
      "type": "object"
    },
    "ThoughtStep": {
      "properties": {
        "thought": {
          "description": "What I'm thinking, e.g. 'Provided: height=174 cm; missing required: blood_type; missing optional: none' or 'User refuses to provide: blood_type'",
          "maxLength": 500,
          "title": "Thought",
          "type": "string"
        },
        "action": {
          "description": "What I'm doing, e.g. 'Save data', 'Ask for required', 'Ask for optional (once)', 'Set skipped'",
          "maxLength": 100,
          "title": "Action",
          "type": "string"
        },
        "action_input": {
          "description": "Data or question, e.g. 'Height: 174 cm' or 'Please provide blood type' or 'Status: skipped due to refusal'",
          "maxLength": 200,
          "title": "Action Input",
          "type": "string"
        }
      },
      "required": [
        "thought",
        "action",
        "action_input"
      ],
      "title": "ThoughtStep",
      "type": "object"
    }
  },
  "additionalProperties": false,
  "properties": {
    "thoughts": {
      "description": "Analysis steps",
      "items": {
        "$ref": "#/$defs/ThoughtStep"
      },
      "title": "Thoughts",
      "type": "array"
    },
    "status": {
      "$ref": "#/$defs/MedScreeningInputStatus",
      "description": "Status: 'incomplete' if missing data, 'complete' when data collected, 'skipped' if skipped"
    },
    "missing_info": {
      "anyOf": [
        {
          "$ref": "#/$defs/MissingInfo"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "List of missing data or null"
    },
    "current_info": {
      "$ref": "#/$defs/SymptomSetup",
      "description": "Current state of collected data"
    }
  },
  "required": [
    "thoughts",
    "status",
    "current_info"
  ],
  "title": "SymptomAnalysisCOT",
  "type": "object",
  "model_name": "SymptomAnalysisCOT"
}
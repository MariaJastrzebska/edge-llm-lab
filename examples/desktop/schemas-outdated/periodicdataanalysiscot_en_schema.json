{
  "$defs": {
    "CreatinineResult": {
      "properties": {
        "value": {
          "description": "Creatinine level in mg/dL",
          "minimum": 0,
          "title": "Value",
          "type": "number"
        },
        "unit": {
          "default": "mg/dL",
          "title": "Unit",
          "type": "string"
        }
      },
      "required": [
        "value"
      ],
      "title": "CreatinineResult",
      "type": "object"
    },
    "ExamTypeEnum": {
      "enum": [
        "glucose test",
        "glycated hemoglobin",
        "lipid profile",
        "creatinine",
        "microalbumin"
      ],
      "title": "ExamTypeEnum",
      "type": "string"
    },
    "GlucoseResult": {
      "properties": {
        "value": {
          "description": "Glucose level in mg/dL",
          "minimum": 0,
          "title": "Value",
          "type": "number"
        },
        "unit": {
          "default": "mg/dL",
          "title": "Unit",
          "type": "string"
        }
      },
      "required": [
        "value"
      ],
      "title": "GlucoseResult",
      "type": "object"
    },
    "HbA1cResult": {
      "properties": {
        "value": {
          "description": "Glycated hemoglobin percentage",
          "maximum": 100,
          "minimum": 0,
          "title": "Value",
          "type": "number"
        },
        "unit": {
          "default": "%",
          "title": "Unit",
          "type": "string"
        }
      },
      "required": [
        "value"
      ],
      "title": "HbA1cResult",
      "type": "object"
    },
    "LipidProfileResult": {
      "properties": {
        "cholesterol_total": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Total cholesterol in mg/dL",
          "title": "Cholesterol Total"
        },
        "cholesterol_hdl": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "HDL cholesterol in mg/dL",
          "title": "Cholesterol Hdl"
        },
        "cholesterol_ldl": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "LDL cholesterol in mg/dL",
          "title": "Cholesterol Ldl"
        },
        "triglycerides": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Triglycerides in mg/dL",
          "title": "Triglycerides"
        },
        "unit": {
          "default": "mg/dL",
          "title": "Unit",
          "type": "string"
        }
      },
      "title": "LipidProfileResult",
      "type": "object"
    },
    "MedScreeningInputStatus": {
      "enum": [
        "incomplete",
        "complete",
        "skipped"
      ],
      "title": "MedScreeningInputStatus",
      "type": "string"
    },
    "MicroalbuminResult": {
      "properties": {
        "value": {
          "description": "Microalbumin level in mg/L",
          "minimum": 0,
          "title": "Value",
          "type": "number"
        },
        "unit": {
          "default": "mg/L",
          "title": "Unit",
          "type": "string"
        }
      },
      "required": [
        "value"
      ],
      "title": "MicroalbuminResult",
      "type": "object"
    },
    "MissingInfo": {
      "properties": {
        "field": {
          "description": "Missing information, e.g. 'height'",
          "maxLength": 50,
          "title": "Field",
          "type": "string"
        },
        "question": {
          "description": "Question to user, e.g. 'What is your height?'",
          "maxLength": 200,
          "title": "Question",
          "type": "string"
        }
      },
      "required": [
        "field",
        "question"
      ],
      "title": "MissingInfo",
      "type": "object"
    },
    "PeriodicDataSetup": {
      "properties": {
        "exam_date": {
          "anyOf": [
            {
              "format": "date",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Exam date - optional",
          "title": "Exam Date"
        },
        "exam_type": {
          "anyOf": [
            {
              "$ref": "#/$defs/ExamTypeEnum"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Exam type - optional"
        },
        "exam_result": {
          "anyOf": [
            {
              "additionalProperties": {
                "anyOf": [
                  {
                    "$ref": "#/$defs/GlucoseResult"
                  },
                  {
                    "$ref": "#/$defs/HbA1cResult"
                  },
                  {
                    "$ref": "#/$defs/LipidProfileResult"
                  },
                  {
                    "$ref": "#/$defs/CreatinineResult"
                  },
                  {
                    "$ref": "#/$defs/MicroalbuminResult"
                  }
                ]
              },
              "propertyNames": {
                "$ref": "#/$defs/ExamTypeEnum"
              },
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Exam result - optional",
          "title": "Exam Result"
        }
      },
      "title": "PeriodicDataSetup",
      "type": "object"
    },
    "ThoughtStep": {
      "properties": {
        "thought": {
          "description": "What I'm thinking, e.g. 'Provided: height=174 cm; missing required: blood_type; missing optional: none' or 'User refuses to provide: blood_type'",
          "maxLength": 500,
          "title": "Thought",
          "type": "string"
        },
        "action": {
          "description": "What I'm doing, e.g. 'Save data', 'Ask for required', 'Ask for optional (once)', 'Set skipped'",
          "maxLength": 100,
          "title": "Action",
          "type": "string"
        },
        "action_input": {
          "description": "Data or question, e.g. 'Height: 174 cm' or 'Please provide blood type' or 'Status: skipped due to refusal'",
          "maxLength": 200,
          "title": "Action Input",
          "type": "string"
        }
      },
      "required": [
        "thought",
        "action",
        "action_input"
      ],
      "title": "ThoughtStep",
      "type": "object"
    }
  },
  "additionalProperties": false,
  "properties": {
    "thoughts": {
      "description": "Analysis steps",
      "items": {
        "$ref": "#/$defs/ThoughtStep"
      },
      "title": "Thoughts",
      "type": "array"
    },
    "status": {
      "$ref": "#/$defs/MedScreeningInputStatus",
      "description": "Status: 'incomplete' if missing data, 'complete' when data collected, 'skipped' if skipped"
    },
    "missing_info": {
      "anyOf": [
        {
          "$ref": "#/$defs/MissingInfo"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "List of missing data or null"
    },
    "current_info": {
      "$ref": "#/$defs/PeriodicDataSetup",
      "description": "Current state of collected data"
    }
  },
  "required": [
    "thoughts",
    "status",
    "current_info"
  ],
  "title": "PeriodicDataAnalysisCOT",
  "type": "object",
  "model_name": "PeriodicDataAnalysisCOT"
}
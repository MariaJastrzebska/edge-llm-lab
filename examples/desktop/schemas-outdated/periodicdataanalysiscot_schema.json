{
  "$defs": {
    "CreatinineResult": {
      "properties": {
        "value": {
          "description": "Poziom kreatyniny w mg/dL",
          "minimum": 0,
          "title": "Value",
          "type": "number"
        },
        "unit": {
          "default": "mg/dL",
          "title": "Unit",
          "type": "string"
        }
      },
      "required": [
        "value"
      ],
      "title": "CreatinineResult",
      "type": "object"
    },
    "ExamTypeEnum": {
      "enum": [
        "test glukozy",
        "hemoglobina glikowana",
        "lipidogram",
        "kreatynina",
        "mikroalbumina"
      ],
      "title": "ExamTypeEnum",
      "type": "string"
    },
    "GlucoseResult": {
      "properties": {
        "value": {
          "description": "Poziom glukozy w mg/dL",
          "minimum": 0,
          "title": "Value",
          "type": "number"
        },
        "unit": {
          "default": "mg/dL",
          "title": "Unit",
          "type": "string"
        }
      },
      "required": [
        "value"
      ],
      "title": "GlucoseResult",
      "type": "object"
    },
    "HbA1cResult": {
      "properties": {
        "value": {
          "description": "Procent hemoglobiny glikowanej",
          "maximum": 100,
          "minimum": 0,
          "title": "Value",
          "type": "number"
        },
        "unit": {
          "default": "%",
          "title": "Unit",
          "type": "string"
        }
      },
      "required": [
        "value"
      ],
      "title": "HbA1cResult",
      "type": "object"
    },
    "LipidProfileResult": {
      "properties": {
        "cholesterol_total": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Całkowity cholesterol w mg/dL",
          "title": "Cholesterol Total"
        },
        "cholesterol_hdl": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Cholesterol HDL w mg/dL",
          "title": "Cholesterol Hdl"
        },
        "cholesterol_ldl": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Cholesterol LDL w mg/dL",
          "title": "Cholesterol Ldl"
        },
        "triglycerides": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Triglicerydy w mg/dL",
          "title": "Triglycerides"
        },
        "unit": {
          "default": "mg/dL",
          "title": "Unit",
          "type": "string"
        }
      },
      "title": "LipidProfileResult",
      "type": "object"
    },
    "MedScreeningInputStatus": {
      "enum": [
        "incomplete",
        "complete",
        "skipped"
      ],
      "title": "MedScreeningInputStatus",
      "type": "string"
    },
    "MicroalbuminResult": {
      "properties": {
        "value": {
          "description": "Poziom mikroalbuminy w mg/L",
          "minimum": 0,
          "title": "Value",
          "type": "number"
        },
        "unit": {
          "default": "mg/L",
          "title": "Unit",
          "type": "string"
        }
      },
      "required": [
        "value"
      ],
      "title": "MicroalbuminResult",
      "type": "object"
    },
    "MissingInfo": {
      "properties": {
        "field": {
          "description": "Brakująca informacja, np. 'height'",
          "maxLength": 50,
          "title": "Field",
          "type": "string"
        },
        "question": {
          "description": "Pytanie do użytkownika, np. 'Jaki jest twój wzrost?'",
          "maxLength": 200,
          "title": "Question",
          "type": "string"
        }
      },
      "required": [
        "field",
        "question"
      ],
      "title": "MissingInfo",
      "type": "object"
    },
    "PeriodicDataSetup": {
      "properties": {
        "exam_date": {
          "anyOf": [
            {
              "format": "date",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Data badania - opcjonalne",
          "title": "Exam Date"
        },
        "exam_type": {
          "anyOf": [
            {
              "$ref": "#/$defs/ExamTypeEnum"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Typ badania - opcjonalne"
        },
        "exam_result": {
          "anyOf": [
            {
              "additionalProperties": {
                "anyOf": [
                  {
                    "$ref": "#/$defs/GlucoseResult"
                  },
                  {
                    "$ref": "#/$defs/HbA1cResult"
                  },
                  {
                    "$ref": "#/$defs/LipidProfileResult"
                  },
                  {
                    "$ref": "#/$defs/CreatinineResult"
                  },
                  {
                    "$ref": "#/$defs/MicroalbuminResult"
                  }
                ]
              },
              "propertyNames": {
                "$ref": "#/$defs/ExamTypeEnum"
              },
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Wynik badania - opcjonalne",
          "title": "Exam Result"
        }
      },
      "title": "PeriodicDataSetup",
      "type": "object"
    },
    "ThoughtStep": {
      "properties": {
        "thought": {
          "description": "Co myślę, np. 'Podano: wzrost=174 cm; brak obowiązkowych: blood_type; brak opcjonalnych: brak' lub 'Użytkownik odmawia podania: blood_type'",
          "maxLength": 500,
          "title": "Thought",
          "type": "string"
        },
        "action": {
          "description": "Co robię, np. 'Zapisz dane', 'Zapytaj o obowiązkowe', 'Zapytaj o opcjonalne (raz)', 'Ustaw skipped'",
          "maxLength": 100,
          "title": "Action",
          "type": "string"
        },
        "action_input": {
          "description": "Dane lub pytanie, np. 'Wzrost: 174 cm' lub 'Podaj grupę krwi' lub 'Status: skipped z powodu odmowy'",
          "maxLength": 200,
          "title": "Action Input",
          "type": "string"
        }
      },
      "required": [
        "thought",
        "action",
        "action_input"
      ],
      "title": "ThoughtStep",
      "type": "object"
    }
  },
  "additionalProperties": false,
  "properties": {
    "thoughts": {
      "description": "Etapy analizy krok po kroku",
      "items": {
        "$ref": "#/$defs/ThoughtStep"
      },
      "title": "Thoughts",
      "type": "array"
    },
    "status": {
      "$ref": "#/$defs/MedScreeningInputStatus",
      "description": "Status: 'incomplete' jeśli brakuje danych, 'complete' gdy dane są zebrane, 'skipped' jeśli pominięto"
    },
    "missing_info": {
      "anyOf": [
        {
          "$ref": "#/$defs/MissingInfo"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Lista brakujących danych lub null"
    },
    "current_info": {
      "$ref": "#/$defs/PeriodicDataSetup",
      "description": "Aktualny stan zebranych danych"
    }
  },
  "required": [
    "thoughts",
    "status",
    "current_info"
  ],
  "title": "PeriodicDataAnalysisCOT",
  "type": "object",
  "model_name": "PeriodicDataAnalysisCOT"
}